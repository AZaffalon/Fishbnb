<h1>liste des réservations</h1>
<table class="table table-striped table-sm">
  <thead>
    <tr>
      <th>Du</th>
      <th>Au</th>
      <th>Disponibilité</th>
    </tr>
  <thead>
  <tbody>
    <% @bookings.each do |booking| %>
      <tr>
        <td><%= booking.start_at %></td>
        <td><%= booking.end_at %></td>
        <td><%= booking.available %></td>
      </tr>
    <% end %>
  </tbody>
</table>
